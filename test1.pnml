<?xml version="1.0" encoding="UTF-8"?>
<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="bigNet" type="http://www.pnml.org/version-2009/grammar/ptnet">

    <!-- ======= PLACES (20 places) ======= -->
    <place id="p1">
      <name><text>P1</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>

    <place id="p2"><name><text>P2</text></name></place>
    <place id="p3"><name><text>P3</text></name></place>
    <place id="p4"><name><text>P4</text></name></place>
    <place id="p5"><name><text>P5</text></name></place>

    <place id="p6">
      <name><text>P6</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>

    <place id="p7"><name><text>P7</text></name></place>
    <place id="p8"><name><text>P8</text></name></place>
    <place id="p9"><name><text>P9</text></name></place>

    <place id="p10">
      <name><text>P10</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>

    <place id="p11"><name><text>P11</text></name></place>
    <place id="p12"><name><text>P12</text></name></place>

    <place id="p13"><name><text>P13</text></name></place>

    <place id="p14">
      <name><text>P14</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>

    <place id="p15"><name><text>P15</text></name></place>

    <place id="p16"><name><text>P16</text></name></place>

    <place id="p17">
      <name><text>P17</text></name>
      <initialMarking><text>1</text></initialMarking>
    </place>

    <place id="p18"><name><text>P18</text></name></place>
    <place id="p19"><name><text>P19</text></name></place>
    <place id="p20"><name><text>P20</text></name></place>


    <!-- ======= TRANSITIONS (10 transitions) ======= -->
    <transition id="t1"><name><text>T1</text></name></transition>
    <transition id="t2"><name><text>T2</text></name></transition>
    <transition id="t3"><name><text>T3</text></name></transition>
    <transition id="t4"><name><text>T4</text></name></transition>
    <transition id="t5"><name><text>T5</text></name></transition>

    <transition id="t6"><name><text>T6</text></name></transition>
    <transition id="t7"><name><text>T7</text></name></transition>
    <transition id="t8"><name><text>T8</text></name></transition>
    <transition id="t9"><name><text>T9</text></name></transition>
    <transition id="t10"><name><text>T10</text></name></transition>


    <!-- ======= ARCS (â‰ˆ 40 arcs) ======= -->

    <!-- Main chain -->
    <arc id="a1"  source="p1"  target="t1"/>
    <arc id="a2"  source="t1"  target="p2"/>
    <arc id="a3"  source="p2"  target="t2"/>
    <arc id="a4"  source="t2"  target="p3"/>
    <arc id="a5"  source="p3"  target="t3"/>
    <arc id="a6"  source="t3"  target="p4"/>
    <arc id="a7"  source="p4"  target="t4"/>
    <arc id="a8"  source="t4"  target="p5"/>

    <!-- Branch 1 -->
    <arc id="a9"  source="p2"  target="t5"/>
    <arc id="a10" source="t5"  target="p6"/>
    <arc id="a11" source="p6"  target="t6"/>
    <arc id="a12" source="t6"  target="p7"/>

    <!-- Branch 2 -->
    <arc id="a13" source="p3"  target="t7"/>
    <arc id="a14" source="t7"  target="p8"/>
    <arc id="a15" source="p8"  target="t8"/>
    <arc id="a16" source="t8"  target="p9"/>

    <!-- Merge -->
    <arc id="a17" source="p7"  target="t9"/>
    <arc id="a18" source="p9"  target="t9"/>
    <arc id="a19" source="t9"  target="p10"/>

    <!-- Loop -->
    <arc id="a20" source="p10" target="t10"/>
    <arc id="a21" source="t10" target="p11"/>
    <arc id="a22" source="p11" target="t1"/>

    <!-- Extra arcs -->
    <arc id="a23" source="p3"  target="t7"/>
    <arc id="a24" source="p4"  target="t5"/>
    <arc id="a25" source="p5"  target="t8"/>
    <arc id="a26" source="p6"  target="t9"/>

    <arc id="a27" source="p11" target="t3"/>
    <arc id="a28" source="p12" target="t4"/>
    <arc id="a29" source="t4"  target="p13"/>
    <arc id="a30" source="p13" target="t6"/>
    <arc id="a31" source="t6"  target="p14"/>

    <arc id="a32" source="p14" target="t1"/>
    <arc id="a33" source="p14" target="t7"/>
    <arc id="a34" source="p7"  target="t2"/>
    <arc id="a35" source="p8"  target="t4"/>

    <arc id="a36" source="p10" target="t6"/>
    <arc id="a37" source="p15" target="t8"/>
    <arc id="a38" source="t8"  target="p16"/>
    <arc id="a39" source="p16" target="t9"/>
    <arc id="a40" source="t9"  target="p17"/>
    <arc id="a41" source="p17" target="t10"/>
    <arc id="a42" source="t10" target="p18"/>
    <arc id="a43" source="p18" target="t3"/>
    <arc id="a44" source="t3"  target="p19"/>
    <arc id="a45" source="p19" target="t5"/>
    <arc id="a46" source="t5"  target="p20"/>

  </net>
</pnml>
